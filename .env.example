# LinkedIn Credentials (use GitHub Secrets in production)
LINKEDIN_EMAIL=your-email@example.com
LINKEDIN_PASSWORD=your-password-here

# Google Sheets
GOOGLE_SHEETS_API_KEY=your-api-key
GOOGLE_SHEETS_ID=your-spreadsheet-id
GOOGLE_SHEETS_JOBS_SHEET=Vagas
GOOGLE_SHEETS_APPLICATIONS_SHEET=Candidaturas

# n8n
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/trigger
N8N_API_KEY=your-n8n-api-key

# Application
NODE_ENV=development
LOG_LEVEL=info
MAX_APPLICATIONS_PER_DAY=50
RETRY_ATTEMPTS=3
RETRY_DELAY_MS=5000

# Browser
HEADLESS=true
BROWSER_TIMEOUT=30000
